@{
    if (ViewContext.RouteData.Values["controller"] != null && (ViewContext.RouteData.Values["controller"]!.ToString() == "Account" || ViewContext.RouteData.Values["controller"]!.ToString() == "Home" || ViewContext.RouteData.Values["controller"]!.ToString() == "Web"))
    {
        Layout = "_Layout";
    }
    else
    {
        Layout = "_NavBarLayout";
    }
}
